{
  "name": "pedicel",
  "version": "0.1.0",
  "description": "",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "files": [
    "lib",
    "README.md"
  ],
  "scripts": {
    "clean": "rimraf dist/*",
    "build": "npm run clean && rollup -c",
    "build:watch": "npm run clean && rollup -c -w",
    "tsc": "tsc",
    "tsc:watch": "tsc -w",
    "format": "prettier --write \"src/**/*.{js,ts,tsx}\"",
    "format:check": "prettier --list-different \"src/**/*.{js,ts,tsx}\"",
    "lint": "tslint -c tslint.json \"src/**/*.{ts,tsx}\"",
    "lint:fix": "tslint -c tslint.json \"src/**/*.{ts,tsx}\" --fix",
    "precommit": "lint-staged",
    "commitmsg": "commitlint -E GIT_PARAMS",
    "release": "standard-version"
  },
  "lint-staged": {
    "{examples_src,src}/**/*.{js,ts,tsx}": [
      "npm run format",
      "npm run lint:fix",
      "git add"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/forsigner/pedicel.git"
  },
  "author": "forsigner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/forsigner/pedicel/issues"
  },
  "homepage": "https://github.com/forsigner/pedicel#README",
  "devDependencies": {
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "@types/react": "^16.4.9",
    "@types/react-dom": "^16.0.7",
    "@types/react-transition-group": "^2.0.13",
    "@types/url-parse": "^1.4.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.64.1",
    "rollup-plugin-commonjs": "^9.1.5",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-typescript2": "^0.16.1",
    "standard-version": "^4.4.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-plugin-prettier": "^1.3.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.3"
  },
  "peerDependencies": {
    "prop-types": "^15.6.2",
    "react": "16.5.1",
    "react-dom": "16.5.1",
    "react-transition-group": "^2.4.0",
    "url-parse": "^1.4.3"
  }
}
